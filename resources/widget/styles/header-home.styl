$calendar = 'data:image/png;base64,'
$time = 'data:image/png;base64,'
$marker = 'data:image/png;base64,'
$de = 'data:image/png;base64,'
$ru = 'data:image/png;base64,'
$en = 'data:image/png;base64,'

.home
  overflow: hidden

.header-event
  display: flex
  justify-content: space-around
  background-color: #003d64
  border: 1px solid #dddedd

  @media (max-width: 767px)
    justify-content: flex-start
    align-items: center

.header-event__link-google
  display: inline-block
  text-decoration: none

  @media (max-width: 767px)
    display: flex
    margin-top: 8px

.header-event__title
  display: flex
  align-items: center
  text-decoration: none
  font-family: 'GothamProBold'
  color: #fff
  font-size: 27px
  line-height: 1.1
  margin: 0

  @media (max-width: 767px)
    font-size: 21px

  @media (max-width: 375px)
    font-size: 18px

  @media (max-width: 325px)
    font-size: 14px

.header-event__text
  margin: 0
  color: #fffc00
  display: inline-block
  font-size: 17px
  line-height: 1.1
  font-weight: 700
  width: calc(100% - 40px)

  @media (max-width: 767px)
    width: calc(100% - 25px)
    font-size: 14px

  @media (max-width: 375px)
    font-size: 10px

.header-event__image
  width: 89px
  height: 89px

  @media (max-width: 767px)
    width: 75px
    margin-left: 9px
    height: 75px

  @media (max-width: 325px)
    margin-left: 0

.header-event__description
  padding: 16px 0
  box-sizing: border-box
  width: 45%
  margin-left: 10px

  @media (max-width: 767px)
    width: 50%

.header-event__date
  padding: 16px 0
  box-sizing: border-box
  width: 40%

.date-container
  display: flex
  align-items: flex-end

  @media (max-width: 767px)
    width: 100%
    justify-content: flex-start

.time-container
  display: flex
  align-items: flex-end

  @media (max-width: 767px)
    width: 100%
    justify-content: flex-start
    margin-top: 11px

.calendar-container
  display: flex
  height: 100%
  justify-content: flex-end
  align-items: center

  @media (max-width: 767px)
    flex-direction: column
    justify-content: center

.calendar__end-date
  display: inline-block
  margin: 0
  color: #333137
  font-weight: 700
  font-size: 13px
  line-height: 11px

.header-event__marker-icon
  display: inline-block
  width: 25px
  height: 25px
  background-image: url($marker)
  background-repeat: no-repeat
  background-size: 25px 25px
  margin-right: 10px

  @media (max-width: 767px)
    margin-right: 4px
    width: 15px
    height: 15px
    background-size: 15px 15px

.calendar__icon
  width: 25px
  height: 25px
  background-image: url($calendar)
  background-repeat: no-repeat
  background-size: 25px 25px
  margin-right: 10px

  @media (max-width: 767px)
    margin-right: 4px
    width: 20px
    height: 20px
    background-size: 20px 20px

  @media (max-width: 375px)
    width: 15px
    height: 15px
    background-size: 15px 15px

.calendar__icon-time
  width: 25px
  height: 25px
  background-image: url($time)
  background-repeat: no-repeat
  background-size: 25px 25px
  margin-right: 10px

  @media (max-width: 767px)
    margin-right: 4px
    width: 20px
    height: 20px
    background-size: 20px 20px

  @media (max-width: 375px)
    width: 15px
    height: 15px
    background-size: 15px 15px

.calendar__end-day
  color: #fff
  font-size: 27px
  line-height: 22px
  font-weight: 700

  @media (max-width: 767px)
    font-size: 26px
    line-height: 19px

  @media (max-width: 375px)
    font-size: 20px
    line-height: 13px

.calendar__end-day-name
  color: #fffc00
  font-size: 13px
  line-height: 11px
  text-transform: capitalize

  @media (max-width: 767px)
    font-size: 12px
    line-height: 9px

  @media (max-width: 375px)
    font-size: 11px
    line-height: 8px

.calendar-year-container
  display: block
  margin-right: 10px

  @media (max-width: 767px)
    margin-right: 4px

.calendar__end-month
  line-height: 13px
  color: #fff
  font-weight: 700
  font-size: 15px
  text-transform: capitalize
  margin-right: 10px

  @media (max-width: 767px)
    margin: 0 6px
    font-size: 13px
    line-height: 9px

  @media (max-width: 375px)
    margin: 0 4px
    font-size: 11px

.calendar__end-year
  line-height: 11px
  color: #fff
  font-size: 13px

  @media (max-width: 767px)
    font-size: 12px
    line-height: 9px

  @media (max-width: 375px)
    font-size: 10px
    line-height: 8px

.calendar__end-hour
  line-height: 22px
  color: #fff
  font-weight: 700
  font-size: 27px

  @media (max-width: 767px)
    font-size: 26px
    line-height: 19px

  @media (max-width: 375px)
    font-size: 20px
    line-height: 13px

.select-language-container
  display: flex
  justify-content: center
  align-items: center
  position: relative
  margin: 0 20px

  @media (max-width: 767px)
    align-items: flex-end
    margin: 0 0 0 19px

  @media (max-width: 375px)
    margin: 0 0 0 4px

.select-language
  display: flex
  justify-content: center
  align-items: center
  position: relative
  cursor: pointer

  &:before
    content: "▼"
    position: absolute
    color: #fff
    left: 100%
    display: block

.select-language-change
  border: 1px solid #DCDCDC
  background-color: #fff
  padding: 10px
  box-sizing: border-box
  position: absolute
  z-index: 2
  top: calc(100% + 5px)

  .select-language__language:first-child
    margin-top: 0

.select-language__language
  margin-top: 10px
  cursor: pointer
  border: 1px solid #000

.select-language-change__en
  width: 25px
  height: 15px
  background-image: url($en)
  background-repeat: no-repeat
  background-size: 100%
  background-position: center

.select-language-change__de
  width: 25px
  height: 15px
  background-image: url($de)
  background-repeat: no-repeat
  background-size: 100%
  background-position: center

.select-language-change__ru
  width: 25px
  height: 15px
  background-image: url($ru)
  background-repeat: no-repeat
  background-size: 100%
  background-position: center

